{
	"manifest_version": 2,
	"name": "OpenTabsCount",
	"description": "Displays the total number of open tabs",
	"version": "1.0",
	"background": {
		"scripts": [
			"background.js"
		]
	},
	"browser_action": {
		"default_icon": "icon.png"
	},
	"permissions": [
		"activeTab",
		"tabs",
		"https://ajax.googleapis.com/"
	],
	"web_accessible_resources": [
		"icon.png"
	]
}